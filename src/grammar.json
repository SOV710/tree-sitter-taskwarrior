{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "taskwarrior",
  "rules": {
    "source_file": {
      "type": "SEQ",
      "members": [
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "_line"
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "CHOICE",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "_statement"
                },
                {
                  "type": "SYMBOL",
                  "name": "comment_line"
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "_line": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SEQ",
          "members": [
            {
              "type": "SYMBOL",
              "name": "_statement"
            },
            {
              "type": "SYMBOL",
              "name": "newline"
            }
          ]
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "SYMBOL",
              "name": "comment_line"
            },
            {
              "type": "SYMBOL",
              "name": "newline"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "newline"
        }
      ]
    },
    "_statement": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "include_statement"
        },
        {
          "type": "SYMBOL",
          "name": "assignment"
        }
      ]
    },
    "comment_line": {
      "type": "SYMBOL",
      "name": "comment"
    },
    "include_statement": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "include"
        },
        {
          "type": "FIELD",
          "name": "target",
          "content": {
            "type": "SYMBOL",
            "name": "include_target"
          }
        }
      ]
    },
    "include_target": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "[^\\s\\r\\n#]+"
      }
    },
    "assignment": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "key",
          "content": {
            "type": "SYMBOL",
            "name": "key"
          }
        },
        {
          "type": "STRING",
          "value": "="
        },
        {
          "type": "FIELD",
          "name": "value",
          "content": {
            "type": "SYMBOL",
            "name": "value"
          }
        }
      ]
    },
    "key": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "[A-Za-z_][A-Za-z0-9_.-]*"
      }
    },
    "value": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "comma_list"
        },
        {
          "type": "SYMBOL",
          "name": "atom_list"
        }
      ]
    },
    "comma_list": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "item",
          "content": {
            "type": "SYMBOL",
            "name": "list_item"
          }
        },
        {
          "type": "STRING",
          "value": ","
        },
        {
          "type": "FIELD",
          "name": "item",
          "content": {
            "type": "SYMBOL",
            "name": "list_item"
          }
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": ","
              },
              {
                "type": "FIELD",
                "name": "item",
                "content": {
                  "type": "SYMBOL",
                  "name": "list_item"
                }
              }
            ]
          }
        }
      ]
    },
    "atom_list": {
      "type": "REPEAT1",
      "content": {
        "type": "SYMBOL",
        "name": "value_atom"
      }
    },
    "list_item": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "quoted_string"
        },
        {
          "type": "SYMBOL",
          "name": "bare_item"
        }
      ]
    },
    "value_atom": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "quoted_string"
        },
        {
          "type": "SYMBOL",
          "name": "bare_atom"
        }
      ]
    },
    "quoted_string": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "\""
          },
          {
            "type": "REPEAT",
            "content": {
              "type": "CHOICE",
              "members": [
                {
                  "type": "PATTERN",
                  "value": "[^\"\\\\\\r\\n]"
                },
                {
                  "type": "PATTERN",
                  "value": "\\\\."
                }
              ]
            }
          },
          {
            "type": "STRING",
            "value": "\""
          }
        ]
      }
    },
    "bare_atom": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "[^\\s,\\r\\n#]+"
      }
    },
    "bare_item": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "[^\\s,\\r\\n#]+"
      }
    },
    "comment": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "#"
          },
          {
            "type": "PATTERN",
            "value": "[^\\r\\n]*"
          }
        ]
      }
    },
    "newline": {
      "type": "PATTERN",
      "value": "\\r?\\n"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "[ \\t]+"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}