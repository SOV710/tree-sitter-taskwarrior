{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "taskwarrior",
      "camelcase": "Taskwarrior",
      "title": "Taskwarrior",
      "scope": "source.taskwarrior",
      "file-types": [
        "taskrc",
        ".taskrc"
      ],
      "highlights": "queries/highlights.scm",
      "injection-regex": "^taskwarrior$",
      "class-name": "TreeSitterTaskwarrior"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Tree-sitter grammar for Taskwarrior configuration files (taskrc)",
    "authors": [
      {
        "name": "SOV710",
        "email": "chris916911179@outlook.com",
        "url": "https://github.com/SOV710"
      }
    ],
    "links": {
      "repository": "https://github.com/SOV710/tree-sitter-taskwarrior"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}
